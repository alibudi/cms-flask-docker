#!/home/ali/PycharmProjects/Web01/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'field==0.2.0','console_scripts','field'
__requires__ = 'field==0.2.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('field==0.2.0', 'console_scripts', 'field')()
    )
